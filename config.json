{
    // General settings
    "uuid_prefix": "Hargassner_",    // Used to create the UUIDs of each channel
    "group_title": "Hargassner",    // Name of the group used in the vz frontend
    
    // Defaults if no specific configuration exists for a channel
    "default_channel_config": {
        "title_prefix": "",             // Title prefix shown in the vz frontend
        "min_intervall": 1.0,           // Minimum intervall between two entries of a channel [s]
        "vz": {                         // vz channel configuration
            "entities": {
                "uuid": "",                 // Will be autogenerated
                "type": "universalsensor",  // Entity type e.g. universalsensor, temperature, ... (see https://wiki.volkszaehler.org/development/definitions )
                "class": "channel"          // Only "channel" is supported
            },
            "properties": {
                "unit": "",                 // Will be automatically filled
                "color": "aqua",            // see https://wiki.volkszaehler.org/software/middleware/einrichtung
                "style": "states",          // see https://wiki.volkszaehler.org/software/middleware/einrichtung
                "fillstyle": 0,             // see https://wiki.volkszaehler.org/software/middleware/einrichtung
                "yaxis": "auto",            // see https://wiki.volkszaehler.org/software/middleware/einrichtung
                "public": 0                 // see https://wiki.volkszaehler.org/software/middleware/einrichtung
            }
        }
    }
}
